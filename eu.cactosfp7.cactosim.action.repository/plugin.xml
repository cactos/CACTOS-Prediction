<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
  <extension point="org.modelversioning.emfprofile.profile">
	<profile profile_resource="transientstates/cactosOptimisationInProgress.emfprofile_diagram"/>
  </extension>
  <extension
         point="org.eclipse.m2m.qvt.oml.javaBlackboxUnits">
      <unit
            name="CactosActionBlackboxUnit"
            namespace="eu.cactosfp7.cactosim.action.repository.black">
         <library
               class="eu.cactosfp7.cactosim.action.repository.black.CactosActionBlackboxLibrary"
               name="CactosActionBlackboxLibrary">
            <metamodel
                  nsURI="http://simulizar.palladiosimulator.org/Actions/Context/1.1">
            </metamodel>
            <metamodel
                  nsURI="http://www.cactosfp7.eu/PhysicalDCModel/Util/1.0">
            </metamodel>
         </library>
      </unit>
   </extension>
   <extension
         point="org.eclipse.m2m.qvt.oml.runtime.qvtTransformation">
      <transformation
            file="cactosActionLibrary/connectApplication.qvto"
            id="cactosActionLibrary.connectApplication">
      </transformation>
   </extension>
   <extension
         point="org.eclipse.m2m.qvt.oml.runtime.qvtTransformation">
      <transformation
            file="cactosActionLibrary/loadBalancerHandling.qvto"
            id="cactosActionLibrary.loadBalancerHandling">
      </transformation>
   </extension>
  <extension
         point="org.eclipse.m2m.qvt.oml.runtime.qvtTransformation">
      <transformation
            file="cactosActionLibrary/createUnassignedVMs.qvto"
            id="cactosActionLibrary.createUnassignedVMs">
      </transformation>
   </extension>
   <extension
         point="org.eclipse.m2m.qvt.oml.runtime.qvtTransformation">
      <transformation
            file="cactosActionLibrary/userBehaviour.qvto"
            id="cactosActionLibrary.userBehaviour">
      </transformation>
   </extension>
  <extension
         point="org.eclipse.m2m.qvt.oml.runtime.qvtTransformation">
      <transformation
            file="cactosActionLibrary/startVM.qvto"
            id="cactosActionLibrary.startVM">
      </transformation>
   </extension>
   <extension
         point="org.eclipse.m2m.qvt.oml.runtime.qvtTransformation">
      <transformation
            file="cactosActionLibrary/placeVMs.qvto"
            id="cactosActionLibrary.placeVMs">
      </transformation>
   </extension>
   <extension
         point="org.eclipse.m2m.qvt.oml.runtime.qvtTransformationContainer">
      <sourceContainer
            path="cactosActionLibrary/">
      </sourceContainer>
   </extension>
   <extension
         point="org.eclipse.m2m.qvt.oml.runtime.qvtTransformationContainer">
      <sourceContainer
            path="adaptation/">
      </sourceContainer>
   </extension>
   <extension
         point="org.eclipse.m2m.qvt.oml.runtime.qvtTransformationContainer">
      <sourceContainer
            path="controllercompletions/">
      </sourceContainer>
   </extension>
   <extension
         point="org.eclipse.m2m.qvt.oml.runtime.qvtTransformationContainer">
      <sourceContainer
            path="precondition/">
      </sourceContainer>
   </extension>
   
   
</plugin>
